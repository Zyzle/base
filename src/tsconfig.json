    "app/+home/index.ts",
    "app/+home/shared/index.ts",
    "app/+login/index.ts",
    "app/+login/login.component.ts",
    "app/+login/login.routes.ts",
    "app/+snippets/+add/add.component.ts",
    "app/+snippets/+add/index.ts",
